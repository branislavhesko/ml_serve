    "version": "0.2.0",
    "configurations": [
    
        {
            "name": "MLHousekeeper",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/ml_housekeeper/ml_housekeeper/server.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "args": [
                "--model_file",
                "${workspaceFolder}/example_model.mlkeep",
                "--host",
                "localhost",
                "--port",
                "5555"
            ]
        }
    ]
}